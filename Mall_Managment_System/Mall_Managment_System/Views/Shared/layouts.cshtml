	<script src="~/web/dist/js/jquery.hover3d.js"></script>
	<script>
		$(document).ready(function () {
			$(".project").hover3d({
				selector: ".project__card"
			});

			$(".movie").hover3d({
				selector: ".movie__card",
				shine: true,
				sensitivity: 20,
			});
		});

	</script>





